<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/button_shape_stroke">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Space
            android:layout_width="match_parent"
            android:layout_height="5dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Id:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvIdImovelLista"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text=" 1"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Localidade:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvLocalidadeImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Localidade"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Quarteirão:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvQuarteiraoImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Quarteirão"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Logradouro:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvLogradouroImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Logradouro"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Número:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvNumeroImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Número"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bairro:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvBairroImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Bairro"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tipo de imóvel:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvtipoImovelImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Tipo de imóvel"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Complemento:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvComplementoImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Complemento"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sequência:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvSequenciaImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Sequência"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Telefone residencial:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvTelefoneResidencialImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Telefone residencial"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Telefone recado:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvTelefoneRecadoImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Telefone recado"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Observação:"
                android:textColor="@color/white"/>

            <TextView
                android:id="@+id/txvObservacaoImovelLista"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Observação"
                android:textColor="@color/white"/>
        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="5dp"/>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>